#!/bin/bash
CLOSURE_LIB=$HOME/svn/closure-library-read-only/closure

python $CLOSURE_LIB/bin/calcdeps.py\
    -p $CLOSURE_LIB\
    -p vendor/closure-templates\
    -p static/js\
    -i static/js/require.js\
    -c vendor/closure.jar\
    -f --create_source_map\
    -f ./lexigraph_map.txt\
    -o compiled > static/js/lexigraph.min.js
    #-f --compilation_level\
    #-f ADVANCED_OPTIMIZATIONS\
